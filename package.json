{
  "name": "fit-check",
  "version": "1.4.0",
  "author": "Harry Evans",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build",
    "typecheck": "tsc --pretty --noEmit",
    "prettier:check": "prettier --check .",
    "lint:check": "eslint . --ext ts --ext tsx --ext js",
    "lint": "next lint",
    "lint:fix": "prettier --write --ignore-path ./.prettierignore . && eslint --cache --fix .",
    "check-all": "yarn build && yarn prettier:check && yarn lint:check && yarn typecheck",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "dev": "next dev -p 3000",
    "open": "open http://localhost:3000",
    "prepare": "husky install",
    "gen:types": "graphql-codegen --config codegen.ts --require dotenv/config",
    "dev:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "madge": "madge --circular --extensions ts,tsx,js,jsx src"
  },
